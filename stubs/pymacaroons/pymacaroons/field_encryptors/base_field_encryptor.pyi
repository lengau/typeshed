class BaseFieldEncryptor:
    nonce: bytes | None
    def __init__(self, signifier: str | None = None, nonce: bytes | None = None) -> None: ...
    @property
    def signifier(self) -> bytes: ...
    @signifier.setter
    def signifier(self, string_or_bytes: str | bytes) -> None: ...
    def encrypt(self, signature: bytes, field_data: str | bytes) -> bytes: ...
    def decrypt(self, signature: bytes, field_data: str | bytes) -> str: ...
